{
  "name": "stock",
  "version": "1.0.0",
  "description": "A React stock visualizer using Markit's API",
  "main": "server.js",
  "scripts": {
    "build": "webpack",
    "build:modules": "webpack --display-modules",
    "watch:webpack": "webpack --watch",
    "refresh": "NODE_ENV=test nodemon server.js",
    "refresh:windows": "set NODE_ENV=test&&nodemon server.js",
    "dev": "npm-run-all --parallel watch:webpack refresh",
    "dev:windows": "npm-run-all --parallel watch:webpack refresh:windows",
    "serve": "NODE_ENV=production node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidLi-Minxiao/stock.git"
  },
  "keywords": [
    "react",
    "universal",
    "node",
    "express"
  ],
  "author": "DavidLi-Minxiao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DavidLi-Minxiao/stock/issues"
  },
  "homepage": "https://github.com/DavidLi-Minxiao/davidli-3#readme",
  "dependencies": {
    "axios": "~0.21.1",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-react": "~6.23.0",
    "babel-preset-stage-0": "~6.22.0",
    "babel-preset-stage-2": "~6.22.0",
    "babel-register": "~6.23.0",
    "compression": "~1.6.2",
    "d3-axis": "~1.0.6",
    "d3-path": "~1.0.5",
    "d3-scale": "~1.0.5",
    "d3-selection": "~1.0.5",
    "d3-selection-multi": "~1.0.1",
    "d3-shape": "~1.0.6",
    "dotenv": "~4.0.0",
    "express": "~4.17.3",
    "lodash": "~4.17.4",
    "milligram": "~1.3.0",
    "preact": "~7.2.1",
    "preact-compat": "~3.14.3",
    "prop-types": "~15.5.8",
    "randomcolor": "~0.4.4",
    "react": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-faux-dom": "~3.0.1",
    "react-onclickoutside": "~5.9.0",
    "react-redux": "~5.0.3",
    "react-router-dom": "4.0.0",
    "redux": "~3.6.0",
    "serve-favicon": "~2.4.2"
